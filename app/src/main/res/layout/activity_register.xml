<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#383838"
    tools:context=".Register">

    <TextView

        android:id="@+id/textView5"
        android:layout_width="135dp"
        android:layout_height="53dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="230dp"
        android:layout_marginBottom="67dp"
        android:clickable="false"

        android:cursorVisible="false"
        android:gravity="center|right"
        android:hint="FULL NAME"
        android:textColor="#efe4b0"
        android:textColorHighlight="#efe4b0"
        android:textColorHint="#efe4b0"
        android:textColorLink="#efe4b0"

        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/full_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/full_name"
        android:layout_width="283dp"
        android:layout_height="67dp"
        android:layout_marginTop="223dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="53dp"
        android:drawableLeft="@android:drawable/ic_input_add"
        android:ems="10"
        android:gravity="center|center_horizontal|left"
        android:hint="Full Name"
        android:inputType="textPersonName"

        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/white"
        android:textColorHint="@android:color/white"
        android:textColorLink="@android:color/white"
        android:textCursorDrawable="@android:color/white"
        app:layout_constraintBottom_toTopOf="@+id/code_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.45" />

    <TextView

        android:id="@+id/textView4"
        android:layout_width="135dp"
        android:layout_height="53dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="67dp"
        android:clickable="false"
        android:cursorVisible="false"
        android:editable="false"
        android:gravity="center|right"
        android:hint="CODE NAME"
        android:text="CODE NAME"
        android:textColor="#efe4b0"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/code_name"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/code_name"
        android:layout_width="283dp"
        android:layout_height="67dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="53dp"
        android:drawableLeft="@android:drawable/star_on"
        android:ems="10"
        android:hint="Create unique Code Name"
        android:inputType="textPersonName"
        android:textAlignment="viewStart"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/white"
        android:textColorHint="@android:color/white"
        android:textColorLink="@android:color/white"
        android:textCursorDrawable="@android:color/white"
        app:layout_constraintBottom_toTopOf="@+id/register_email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/full_name" />

    <TextView

        android:id="@+id/textView3"
        android:layout_width="135dp"
        android:layout_height="53dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="65dp"
        android:clickable="false"
        android:cursorVisible="false"
        android:editable="false"
        android:gravity="center|right"
        android:hint="Full Name"
        android:text="EMAIL ID"
        android:textColor="#efe4b0"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/register_email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/register_email"
        android:layout_width="283dp"
        android:layout_height="67dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="52dp"
        android:drawableLeft="@android:drawable/sym_action_email"
        android:ems="10"
        android:hint="Enter valid e-mail"
        android:inputType="textPersonName"
        android:textAlignment="viewStart"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/white"
        android:textColorHint="@android:color/white"
        android:textColorLink="@android:color/white"
        android:textCursorDrawable="@android:color/white"
        app:layout_constraintBottom_toTopOf="@+id/create_password"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/code_name"
        tools:ignore="MissingConstraints" />

    <TextView

        android:id="@+id/textView2"
        android:layout_width="135dp"
        android:layout_height="53dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="220dp"
        android:clickable="false"
        android:cursorVisible="false"
        android:editable="false"
        android:gravity="center|right"
        android:text="PASSWORD"
        android:textColor="#efe4b0"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/create_password"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/create_password"
        android:layout_width="283dp"
        android:layout_height="67dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="80dp"
        android:drawableLeft="@android:drawable/ic_lock_lock"
        android:ems="10"
        android:hint="Create Password"
        android:inputType="textPassword"
        android:textAlignment="viewStart"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/white"
        android:textColorHint="@android:color/white"
        android:textColorLink="@android:color/white"
        android:textCursorDrawable="@android:color/white"
        app:layout_constraintBottom_toTopOf="@+id/register_Button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/register_email"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/register_Button"
        android:layout_width="204dp"
        android:layout_height="55dp"
        android:layout_marginStart="89dp"
        android:layout_marginEnd="139dp"
        android:layout_marginBottom="99dp"
        android:background="#424040"
        android:text="register"
        android:textColor="#efe4b0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/create_password"
        tools:ignore="MissingConstraints" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyle"
        android:layout_width="93dp"
        android:layout_height="62dp"
        android:layout_marginTop="72dp"
        android:layout_marginBottom="94dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/create_password" />

</androidx.constraintlayout.widget.ConstraintLayout>